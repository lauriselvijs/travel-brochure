    "bootstrap-icons": "^1.10.3",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.2",
    "dotenv": "^16.0.3",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "imagemin": "^8.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "jquery": "^3.6.3",
    "mapbox-gl": "^2.12.0",
    "popper.js": "^1.16.1",
